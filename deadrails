local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()  

local Window = Rayfield:CreateWindow({
    Name = "DEAD OR ALIVE",
    Icon = 76514024748649,
    LoadingTitle = "DEATH OR ALIVE [UNIVERSAL]",
    LoadingSubtitle = "by scorp",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "Big Hub"
    },
    Discord = {
        Enabled = false,
        Invite = "noinvitelink",
        RememberJoins = true
    },
    KeySystem = true,
    KeySettings = {
        Title = "Key system",
        Subtitle = "Dead or alive key system",
        Note = "Key in telegram @scorpbf",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = false,
        Key = {"doa_134", "doa_135", "doa_175", "doa_762"}
    }
})

-- Создание вкладок
local InfoTab = Window:CreateTab("Info", 4483362458)
local ScriptsTab = Window:CreateTab("Scripts", 4483362459)  -- Вкладка "Scripts"
local PlayerTab = Window:CreateTab("Player", 4483362460)  -- Вкладка "Player"
local VisualTab = Window:CreateTab("Visual", 4483362461)  -- Вкладка "Visual"

